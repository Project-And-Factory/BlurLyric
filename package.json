  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gozaoo/BlurLyric2.0/issues"
  },
  "homepage": "https://github.com/gozaoo/BlurLyric2.0#readme",
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "BlurLyric",
        "icon": "./icon.ico",
        "setupIcon": "./icon.ico",
        "authors": "Doge"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "blurlyric"
          }
