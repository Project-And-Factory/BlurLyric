    "md5": "^2.3.0",
    "music-metadata": "^7.5.3",
    "pac-proxy-agent": "^5.0.0",
    "plyr": "^3.7.2",
    "qrcode": "^1.5.0",
    "rgbaster": "^2.1.1",
